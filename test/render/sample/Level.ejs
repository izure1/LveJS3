<!DOCTYPE HTML>
<html>

<head>
  <title></title>
  <link rel="stylesheet" href="/css/common.css">
  <meta charset="utf-8">
  <script type="text/javascript" src="/LveJS.js"></script>
</head>

<body>
  <canvas id="test" width="800" height="450"></canvas>
  <div>
    <button onclick="lve('camera').attr('level', 'main')">Main</button>
    <button onclick="lve('camera').attr('level', 'main::inner')">Main::inner</button>
    <button onclick="lve('camera').attr('level', 'another')">another</button>
  </div>
  <script type="text/javascript">
    const lve = new LveJS

    lve.init({
      canvas: '#test'
    }).loadAsset('/video/sample.mp4').ready(() => {

      lve('video').create({
        type: 'video',
        src: '/video/sample.mp4'
      }).css('verticalAlign', 'middle').play()
      
    })
  </script>
</body>

</html>